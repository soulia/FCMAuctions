@*@model IEnumerable<FCMAuction.Models.ItemBid>*@
@model IEnumerable<BidListViewModel>

<table>
    <tr>
        <th>
            Bids
        </th>
        <th>
            Id
        </th>
        <th></th>
    </tr>
    @if(Model.Any())
    { 
    foreach (var bid in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => bid.Bid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => bid.ItemId)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id = item.Id })*@ 
            </td>
        </tr>
    }
    }

</table>

